{
  "name": "@quicksprout/lambda",
  "version": "1.0.0-40",
  "description": "Run headless Chrome/Chromium on AWS Lambda",
  "author": "Marco LÃ¼thy",
  "contributors": ["Steve Cox @ Quick Sprout"],
  "keywords": [
    "serverless",
    "chrome",
    "chromium",
    "headless",
    "aws",
    "lambda",
    "puppeteer"
  ],
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.es.js",
  "files": [
    "dist/bundle.cjs.js",
    "dist/bundle.es.js",
    "scripts/postinstall.js"
  ],
  "repository": "git@github.com:quicksprout/serverless-chrome.git",
  "bugs": {
    "url": "https://github.com/quicksprout/serverless-chrome/issues"
  },
  "homepage": "https://github.com/quicksprout/serverless-chrome/tree/master/packages/lambda",
  "license": "MIT",
  "engines": {
    "node": ">= 6.10"
  },
  "config": {
    "jsSrc": "src/"
  },
  "scripts": {
    "clean": "rm -Rf dist/ ./**.zip",
    "test": "npm run test:integration",
    "test:integration": "scripts/test-integration.sh",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run clean && npm run build",
    "postinstall": "node scripts/postinstall.js",
    "package-binaries": "scripts/package-binaries.sh",
    "latest-browser-versions": "scripts/latest-versions.sh",
    "upgrade-dependencies": "yarn upgrade-interactive --latest --exact"
  },
  "dependencies": {
    "extract-zip": "1.6.6",
    "puppeteer": "^1.2.0"
  },
  "devDependencies": {
    "ava": "0.24.0",
    "babel-core": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "chrome-launcher": "0.10.0",
    "rollup": "0.53.3",
    "rollup-plugin-babel": "3.0.3",
    "rollup-plugin-node-resolve": "3.0.0"
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      [
        "env",
        {
          "modules": "commonjs",
          "targets": {
            "node": "6.10"
          }
        }
      ]
    ]
  }
}
